<%- include('partials/header') %>

<div class="container bg-light p-5 w-25 h3 shadow-lg" id="quizMasterContainer">
  <div class="text-center"><h5>Q B M S</h5></div>
  <div class="text-center"><p>[Question Bank Management System]</p></div>
  <hr>
  <div class="quizBody" id="quizBody">
    <form action="/stream" method="post">
    <div class="form-check p-2 px-4">
      <!-- <input class="form-check-input" type="radio" name="QUIZTYPE" value="topicTest" id="1" checked> -->
      <input class="form-check-input" type="radio" name="STREAM" value="iitjee" id="1" checked>
      <label class="form-check-label" for="1">
        IIT-JEE
      </label>
    </div>
    <div class="form-check p-2 px-4">
      <input class="form-check-input" type="radio" name="STREAM" value="neet" id="2" >
      <label class="form-check-label" for="2">
        NEET
      </label>
    </div>
    <div class="form-check p-2 px-4">
      <input class="form-check-input" type="radio" name="STREAM" value="foundation" id="3">
      <label class="form-check-label" for="3">
        FOUNDATION
      </label>
    </div>
    <div class="form-check p-2 px-4">
        <input class="form-check-input" type="radio" name="STREAM" value="miscellaneous" id="4">
        <label class="form-check-label" for="4">
        MISCELLANEOUS
        </label>
      </div>
      <button class="text-white p-1 pt-2 mt-3 w-100" id='submitBtn'>Submit</button>
    </form>
</div>

<%- include('partials/footer') %>